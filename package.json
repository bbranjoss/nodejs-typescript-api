{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "ts-node-dev src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bbranjoss/nodejs-typescript-api.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Bruno Anjos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bbranjoss/nodejs-typescript-api/issues"
  },
  "homepage": "https://github.com/bbranjoss/nodejs-typescript-api#readme",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.9.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "jest": "^27.4.5",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "module-alias": "^2.2.2"
  }
}
